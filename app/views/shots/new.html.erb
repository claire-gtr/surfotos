<div class="container">
  <div class="row">
    <div class="col">
      <%= simple_form_for @shot do |f| %>
        <%= f.input :photo %>
        <%= f.input :photo_cache, as: :hidden %>
        <%= f.input :shot_on, as: :datetime %>
        <%= f.input :price_cents %>
        <%= f.button :submit %>
      <% end %>
    </div>
  </div>
</div>
